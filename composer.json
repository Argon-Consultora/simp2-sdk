{
    "name": "simp2/sdk",
    "description": "SDK para comunicarse con el simp2 desde un conector",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.10",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.3",
        "bensampo/laravel-enum": "^6.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "authors": [
        {
            "name": "ArgonConsultora",
            "homepage": "https://argonconsultora.com.ar"
        }
    ],
    "autoload": {
        "psr-4": {
            "SIMP2\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SIMP2\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SIMP2\\SDK\\SIMP2ServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --provider=\"SIMP2\\SDK\\SIMP2ServiceProvider\""
        ],
        "post-autoload-dump": [

        ]
    }
}
